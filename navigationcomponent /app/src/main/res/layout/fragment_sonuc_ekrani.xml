        android:layout_marginEnd="32dp"
        tools:text="TextView"
        android:textSize="20dp"
        android:textColor="#FF5722"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
